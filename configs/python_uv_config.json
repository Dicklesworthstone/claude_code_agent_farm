{
  "comment": "Advanced configuration for Python projects using uv package manager",
  "tech_stack": "python",
  "problem_commands": {
    "type_check": ["uv", "run", "mypy", "src", "--strict"],
    "lint": ["uv", "run", "ruff", "check", "src", "tests", "--fix"]
  },
  "best_practices_files": [],
  "agents": 15,
  "session": "claude_agents",
  "stagger": 12.0,
  "wait_after_cc": 20.0,
  "check_interval": 15,
  "skip_regenerate": false,
  "skip_commit": false,
  "auto_restart": true,
  "no_monitor": false,
  "attach": false,
  "prompt_file": "prompts/default_prompt_python.txt",
  "context_threshold": 25,
  "idle_timeout": 90,
  "max_errors": 5,
  "git_branch": "main",
  "git_remote": "origin",
  "tmux_kill_on_exit": true,
  "tmux_mouse": true,
  "fast_start": false,
  "full_backup": false,
  "max_agents": 30
} 